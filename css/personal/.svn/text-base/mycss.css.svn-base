body{
	background-color:white;
}
.mui-content{
	background-color:white;
}
#myheader{
	background-color:transparent;
	box-shadow: none;
}
.headimg{
	width: 100%;
	padding:0.44rem 0;
    background-image: url(../../images/196.jpg);
    background-size:100% 100%;  
    text-align: center; 
}
.headimg img{
	width: 1rem;
	height:1rem;
	border-radius: 0.5rem;
}
.headimg .mui-content-padded p{
	font-size:16px;
	color:black;
}

.content{
	text-align:left;
	padding-left:0.1rem;
	padding-right:0.1rem;
	font-size:15px;
}

.content .mui-row{
	line-height:0.6rem;
	border-bottom: 1px solid #efefef;
}
.content .mui-row,.content .mui-row a,.content .oner h3,.content .mui-row p{
	color:#444444;
}
.content .oner h3{
	font-size:18px;
	line-height:0.6rem;
	font-weight: normal;
	margin:0;
}
.content .oner .all{
	color:#584e4e;
}
.content .twor{
	text-align:center;
}
.content .twor img{
	width:0.3rem;
	height:0.3rem;
	margin-top:0.2rem;
}
.content .twor p{
	line-height:0.4rem;
}
.content .twor .first{
	border-right: 1px solid #efefef;
}



